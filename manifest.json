{
  "name": "Wikipedia with MathJax",
  "version": "0.0.4",
  "manifest_version": 2,
  "description": "Renders Wikipedia equations with MathJax",
  "icons": {
    "16": "wiki-mjax-16.png",
    "48": "wiki-mjax-48.png",
    "128": "wiki-mjax-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*.wikipedia.org/*", "https://*.wikipedia.org/*"],
      "js": ["jquery-min-1.7.2.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
